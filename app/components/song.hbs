<dl ...attributes>
    <dt>
        {{#if this.url}}
            <a href={{this.url}}>
                {{@song.title}}
            </a>
        {{else}}
            {{@song.title}}
        {{/if}}
    </dt>
    {{#if @song.pad}}
        <dd><strong>ü•Å {{@song.pad}}</strong></dd>
    {{/if}}
    {{#each @song.notes as |note|}}
        <dd>{{note}}</dd>
    {{/each}}
</dl>